  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: PyInstaller",
      "type": "python",
      "request": "launch",
      "module": "PyInstaller",
      "args": ["debugpy_cli.spec"],
      "cwd":"${workspaceFolder}",
      "justMyCode": false
    },

    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${fileDirname}"
    }
  ]
}
